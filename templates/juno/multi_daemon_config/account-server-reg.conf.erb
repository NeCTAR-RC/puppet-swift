[DEFAULT]
bind_ip = <%= @ipaddress_regnet %>
bind_port = 6002
workers = <%= @workers %>
set log_level = WARN
log_level = WARN

[pipeline:main]
pipeline = recon account-server

[app:account-server]
use = egg:swift#account

[account-auditor]

[account-reaper]

[filter:recon]
use = egg:swift#recon
recon_cache_path = /var/cache/swift
