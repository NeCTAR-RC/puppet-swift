[DEFAULT]
bind_ip = 0.0.0.0
bind_port = 6001
workers = <%= @workers %>
log_facility = LOG_USER

[pipeline:main]
pipeline = recon container-server

[app:container-server]
use = egg:swift#container

[container-replicator]

[container-updater]

[container-auditor]

[filter:recon]
use = egg:swift#recon
recon_cache_path = /var/cache/swift
